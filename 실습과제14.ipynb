    "import re\n",
    "import math\n",
    "\n",
    "def make_square(match):\n",
    "    num= int(match.group())\n",
    "    return str(int(math.pow(num, 2)))\n",
    "\n",
    "print(re.sub(r'\\d+', make_square, \"Please, square the following number, 3 7 11 13 17 19\"))"
